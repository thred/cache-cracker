{
    "compilerOptions": {
        "outDir": "./build/src",
        "rootDir": "./src",
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": true,
        "sourceMap": true,
        "jsx": "react",
        "removeComments": false,
        "preserveConstEnums": true
    },
    "exclude": [
        "backup",
        "build",
        "dist",
        "node_modules",
        "typings"
    ]
}